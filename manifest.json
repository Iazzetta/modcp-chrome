{
  "manifest_version": 2,

  "name": "Modcp Chrome",
  "description": "Painel moderativo para administradores e moderadores de forums.",
  "version": "2.0",

  "browser_action": {
    "default_icon": {
      "19": "/media/icon16.png",
      "38": "/media/icon48.png",
      "64": "/media/icon64.png"
   },
    "default_popup": "/view/app.html",
    "default_title": "4U Painel"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "http://*/*", 
    "https://*/*", 
    "unlimitedStorage", 
    "contextMenus", 
    "cookies", 
    "tabs", 
    "notifications",
    "clipboardWrite",
    "clipboardRead",
    "storage"
  ]

}